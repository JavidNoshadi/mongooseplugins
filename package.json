{
  "author": "Aaron Heckmann <aaron.heckmann+github@gmail.com>",
  "name": "plugins.mongoosejs.com",
  "description": "Mongoosejs plugins site",
  "version": "0.0.1",
  "repository": {
    "url": ""
  },
  "dependencies": {
    "npm": "1.4.9",
    "express": "3.0",
    "forever": "0.15.1",
    "request": "2.9.203",
    "jade": "0.27.0",
    "github-url-from-git": "1.1.1"
  },
  "devDependencies": {
    "switchblade": "0.0.1"
  },
  "optionalDependencies": {},
  "engines": {
    "node": "0.10.28"
  },
  "scripts": {
    "deploy": "switchblade -s start-prod -t stop-prod -h plugins.mongoosejs.io ./*.js ./lib/* ./routes/* ./views/* ./public/* ./package.json",
    "start": "node .",
    "start-prod": "env PORT=80 forever start --append --uid plugins index.js",
    "stop-prod": "forever stop plugins"
  }
}
