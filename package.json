{
  "author": "Aaron Heckmann <aaron.heckmann+github@gmail.com>",
  "name": "plugins.mongoosejs.io",
  "description": "Mongoosejs plugins site",
  "version": "0.0.1",
  "repository": {
    "url": "https://github.com/vkarpov15/mongooseplugins"
  },
  "browser": "./lib/client.js",
  "browserify": {
    "transform": [["babelify", { "presets": ["es2015"] }]]
  },
  "dependencies": {
    "@cycle/core": "6.0.3",
    "@cycle/dom": "9.2.2",
    "@cycle/http": "8.2.2",
    "JSONStream": "1.1.1",
    "co": "4.6.0",
    "express": "4.13.3",
    "forever": "0.15.1",
    "github-url-from-git": "1.1.1",
    "jade": "0.27.0",
    "moment": "2.12.0",
    "mkdirp": "0.5.1",
    "mongodb": "2.1.9",
    "mongodb-topology-manager": "1.0.6",
    "mongodb-version-manager": "1.0.2",
    "ramda": "0.19.1",
    "rx": "4.1.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "6.6.0",
    "babelify": "7.2.0",
    "browserify": "13.0.0",
    "switchblade": "0.0.1"
  },
  "optionalDependencies": {},
  "engines": {
    "node": "0.10.28"
  },
  "scripts": {
    "compile": "browserify -o public/main.js lib/client.js",
    "deploy": "switchblade -s start-prod -t stop-prod -h plugins.mongoosejs.io ./*.js ./lib/* ./routes/* ./views/* ./public/* ./package.json",
    "postinstall": "m use 3.2.3",
    "start": "node .",
    "start-prod": "env PORT=80 forever start --append --uid plugins index.js",
    "stop-prod": "forever stop plugins"
  }
}
